        <form action="/users/cadastro" method="POST" style="margin-top: 200px;" >
        
          <div class="row">
            <div class="col">
              <label for="inputNome">Nome</label>
              <input type="text" class="form-control" placeholder="Nome" id="nome" name="nome" >
            </div>
            <div class="col">
              <label for="inputTelefone">Telefone</label>
              <input type="text" class="form-control" placeholder="telefone com DD" id="telefone" name="telefone">
            </div>
          </div>
          <div class="form-group">
            <label for="inputEmail">Email</label>
            <input type="email" class="form-control" placeholder="email" id="email" name="email">
          </div>
          <div class="form-group">
            <label for="inputSenha">Senha</label>
            <input type="password" class="form-control" placeholder="Digite sua senha" id="senha" name="senha">
          </div>
          <div class="form-row">
            <div class="form-group col-md-6">
              <label for="inputEndereco">Endereço</label>
              <input type="text" class="form-control" placeholder="Digite seu endereço" id="endereco" name="endereco">
            </div>

            <div class="form-group col-md-2">
              <label for="inputCPF">Cpf</label>
              <input type="text" class="form-control" id="inputCEP" id="cpf" name="cpf">
            </div>
          </div>
          <div class="form-group">
            <div class="form-check">
              <input class="form-check-input" type="checkbox" id="gridCheck">
              <label class="form-check-label" for="gridCheck">
                Clique em mim
              </label>
            </div>
          </div>
          <button type="submit" class="btn btn-primary">Entrar</button>
        </form>

            
